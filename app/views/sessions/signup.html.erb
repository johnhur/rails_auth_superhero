<h1>Sessions#signup</h1>
<p>Find me in app/views/sessions/signup.html.erb</p>

<%= form_for(@user, url: signup_path) do |f|%>
<p>
	Username: <%= f.text_field :username, autofocus: true %>
</p>
<p>
	Password: <%= f.text_field :password, required: true %>
</p>
<p>
	<%= f.submit "signup", class: "btn btn-primary" %>
</p>
<%= link_to "Have an account already? Log in here", login_path %>
<% end %>

