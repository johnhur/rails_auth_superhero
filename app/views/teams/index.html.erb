<h1>Teams#index</h1>



<p><%= link_to "Logout", logout_path, method: :delete %></p>
<p><%= link_to "Add Team", new_user_team_path(session[:user_id])%></p>

<h4>Teams:</h4>
<ul>
<% @teams.each do |t| %>
	<li>
		Team Name:<%= t.name %>
		Team Id: <%= t.id %>
		<%= link_to "show", team_path(t)%>
		<%= link_to "edit", edit_team_path(t)%>
		<%= link_to "X", team_path(t), method: :delete %>
	</li>
<% end %>
</ul>